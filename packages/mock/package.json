{
  "name": "@simplix-react/mock",
  "version": "0.1.1",
  "description": "Auto-generated MSW handlers and in-memory stores from @simplix-react/contract",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run --passWithNoTests",
    "clean": "rm -rf dist .turbo"
  },
  "peerDependencies": {
    "@simplix-react/contract": "workspace:*",
    "msw": ">=2.0.0",
    "vite": ">=5.0.0",
    "zod": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "msw": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@simplix-react/config-eslint": "workspace:*",
    "@simplix-react/config-typescript": "workspace:*",
    "@simplix-react/contract": "workspace:*",
    "@types/node": "^25.2.3",
    "eslint": "^9.39.2",
    "msw": "^2.7.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^3.0.0",
    "zod": "^4.0.0"
  }
}
